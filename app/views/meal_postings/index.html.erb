<section class="postings-list">

<% @meal_postings.each do |meal_posting| %>

  <!-- Image -->
  <div class="meal-div">
    <div class="media">
      <div class="media-left media-middle">
        <%= cl_image_tag(meal_posting.media[0].url, :width=>100, :height=>100, class: 'media-object') %>
      </div>

      <div class="media-body">
        <div class="container-fluid media-container">

          <!-- Title, meal time(s), cost -->
          <div class="row row-heading">
            <div class="col-xs-4">
              <%= meal_posting.title %>
            </div>

            <div class="col-xs-4">
            <% if meal_posting.end_time %>
              Serving from: <%= meal_posting.base_time %> - <%= meal_posting.end_time%>
            <% else %>
              Serving at: <%= meal_posting.base_time%>
            <% end %>
            </div>

            <div class="col-xs-4">
              Cost: $<%= meal_posting.cost %>
            </div>
          </div>



          <!-- Description -->
          <div class="row row-body">
            <div class="col-xs-12">
            <%= meal_posting.description %>
            </div>
          </div>

          <!-- Servings left, allergy notices -->
          <div class="row row-footer">
            <div class="col-xs-4">
              Servings Left: <%= meal_posting.servings %>
            </div>
            <div class="col-xs-4">
            </div>
            <div class="col-xs-4">
              Allergies: <%= meal_posting.allergy_notice %>
            </div>
          </div>

        </div>
      </div>

    </div>
  </div>
</br>
<% end %>

</section>



